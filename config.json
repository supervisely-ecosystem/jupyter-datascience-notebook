{
  "name": "Jupyter datascience notebook",
  "type": "app",
  "version": "2.0.0",
  "categories": [
    "development"
  ],
  "description": "Run Jupyterlab server on your computer with Supervisely Agent and access it from anywhere",
  "docker_image": "supervisely/jupyter-datascience-notebook:1.0.0",
  "entrypoint": "jupyter lab --ip=0.0.0.0 --port=8000 --allow-root --ServerApp.notebook_dir='/sly-app-data' --ServerApp.token='' --ServerApp.password='' --ServerApp.allow_origin=* --ServerApp.base_url=$BASE_URL --ServerApp.trust_xheaders=True --ServerApp.disable_check_xsrf=True",
  "port": 8000,
  "proxyKeepUrl": true,
  "task_location": "application_sessions",
  "icon": "https://img.icons8.com/fluency/144/000000/jupyter.png",
  "icon_background": "#FFFFFF",
  "community_agent": false
}
